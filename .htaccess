ErrorDocument 404 /index.php?page=error
RewriteEngine ON
Options +FollowSymlinks -Indexes
RewriteBase /
RewriteRule ^.htaccess$ -[F]
 
RewriteRule ^(index+)/{0,1}$ index.php?page=$1
RewriteRule ^(index+)/(a-z]+)/([0-9]+)/{0,1}$ index.php?&page=$1&news=$2
RewriteRule ^(index+)/(a-z]+)/([0-9]+)/{0,1}$ index.php?&page=$1&user=$2